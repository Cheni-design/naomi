{
  "label": "General",
  "description": "Select general model options:",
  "controlGroups": [
    {
      "label": "Area scope",
      "controls": [
        {
          "name": "area_scope",
          "type": "multiselect",
          "options":
            <area_scope_options>,
          "value": <area_scope_default>,
          "required": true
        }
      ]
    },
    {
      "label": "Area level",
      "controls": [
        {
          "name": "area_level",
          "type": "select",
          "options":
            <area_level_options>,
          "required": true
        }
      ]
    },
    {
      "label": "Calendar quarter at midpoint of survey",
      "controls": [
        {
          "name": "calendar_quarter_t1",
          "type": "select",
          "options":
            <calendar_quarter_t1_options>,
          "required": true
        }
      ]
    },
    {
      "label": "Calendar quarter to generate estimates",
      "controls": [
        {
          "name": "calendar_quarter_t2",
          "type": "select",
          "options":
            <calendar_quarter_t2_options>,
          "required": true
        }
      ]
    }
  ]
}
