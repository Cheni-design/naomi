{
    "label": "Calibration options",
    "description": "Options for calibrating to Spectrum outputs",
    "controlGroups": [
	{
	    "label": "Adjust to Spectrum population size",
	    "controls": [
		{
		    "name": "spectrum_population_calibration",
		    "type": "select",
		    "options":
		    [
			{
			    "id": "none",
			    "label": "None"
			},
			{
			    "id": "national",
			    "label": "National"
			},
			{
			    "id": "subnational",
			    "label": "Subnational"
			}
		    ],
		    "value": "national",
		    "required": true,
		    "helpText": "Calibrate to Spectrum population size by sex and 5-year age group by national or subnational. If national Spectrum file, national and subnational will return the same result."
		}
	    ]
	}
    ]
},
{
