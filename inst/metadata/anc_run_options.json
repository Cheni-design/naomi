{
  "label": "ANC",
  "description": "Optionally select which quarter of data to use at time point 1 and 2",
  "controlGroups": [
    {
      "label": "Prevalence",
      "controls": [
        {
          "name": "anc_prevalence_year1",
          "label": "Time 1",
          "type": "select",
          "helpText": "Year matching midpoint of survey",
          "options":
            <+anc_prevalence_year1_options+>,
          "required": false
        },
        {
          "name": "anc_prevalence_year2",
          "label": "Time 2",
          "type": "select",
          "helpText": "Year to generate estimates for",
          "options":
            <+anc_prevalence_year2_options+>,
          "required": false
        }
      ]
    },
    {
      "label": "ART coverage prior to first ANC",
      "controls": [
        {
          "name": "anc_art_coverage_year1",
          "label": "Time 1",
          "type": "select",
          "helpText": "Year matching midpoint of survey",
          "options":
            <+anc_art_coverage_year1_options+>,
          "required": false
        },
        {
          "name": "anc_art_coverage_year2",
          "label": "Time 2",
          "type": "select",
          "helpText": "Year to generate estimates for",
          "options":
            <+anc_art_coverage_year2_options+>,
          "required": false
        }
      ]
    }
  ]
}
